[commit]
	gpgSign = false
[core]
	abbrev = 8
	bigFileThreshold = 16m
	compression = 7
	deltaBaseCacheLimit = 128m
	looseCompression = 2
[credential]
	helper = cache --timeout=86400
[fetch]
	writeCommitGraph = true
[gc]
	aggressiveDepth = 100
	aggressiveWindow = 500
	auto = 0
	bigPackThreshold = 1g
	pruneExpire = now
[init]
	defaultBranch = main
[maintainance]
	auto = false
[pack]
	compression = 7
	packSizeLimit = 1g
	threads = 4
	useSparse = true
	writeBitmapLookupTable = true
	writeReverseIndex = true
[pull]
	ff = only
[push]
	default = simple
	gpgSign = false
[receive]
	autogc = false
[repack]
	writeBitmaps = true
